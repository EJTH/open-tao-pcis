function score(){
"Afstand fra vagter til nærmeste montre er mindre end x" : Scoring.closestDistance('vagt','montre') < 3,
"Døre og vagter er ikke blokeret" : Scoring.areaTest('door.zone') ? 0 : 1
"Afstanden fra markedspladen til nærmeste montre er mindre end x" : Scoring.areaTest('scene.zone') ? 0 : 1,
"Nærmeste montre til sværdmontre er øksemontren" : Scoring.closerThanOthers('svaerd','montre','oekse'),
"Nærmeste montre til hjelmmontre er skjoldmontren" : Scoring.closerThanOthers('hjelme','montre','skjold'),
"Nærmeste montre til runemontre er vølvemontren" : Scoring.closerThanOthers('runesten','montre','stave'),
"Drikkehornsmontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('horn') || Scoring.pointSnapObjectTest('horn'),
"Øksemontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('oekse') || Scoring.pointSnapObjectTest('oekse'),
"Sværdmontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('svaerd') || Scoring.pointSnapObjectTest('svaerd'),
"Hjelmmontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('hjelm') || Scoring.pointSnapObjectTest('hjelm'),
"Skjoldmontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('skjold') || Scoring.pointSnapObjectTest('skjold'),
"Runemontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('rune') || Scoring.pointSnapObjectTest('rune'),
"Vølvemontres bagside snappet op ad væg eller anden montres bagside" : Scoring.wallSnapObjectTest('stav') || Scoring.pointSnapObjectTest('stav')

// 14. Afstand fra drikkehornsmontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 15. Afstand fra øksemontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 16. Afstand fra sværdmontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 17. Afstand fra hjelmmontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 18. Afstand fra skjoldmontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 19. Afstand fra runemontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
// 20. Afstand fra vølvemontres forside til nærmeste objekt, inkl. Væg, er mindre end x.
}
